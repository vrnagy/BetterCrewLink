    "electron-store": "^6.0.1",
    "electron-updater": "^4.3.5",
    "electron-window-state": "^5.0.3",
    "iohook": "^0.7.2",
    "jsondiffpatch": "^0.4.1",
    "memoryjs": "git://github.com/Rob--/memoryjs",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-spinners-kit": "^1.9.1",
    "react-tooltip-lite": "^1.12.0",
